<% currently_at 'Posts' %>

<h1>Posts</h1>

<%= link_to 'New Post', new_post_path, class: 'btn btn-primary' %>

<table class="table table-striped">
  <thead>
    <tr>
      <th scope="col">#</th>
      <th scope="col">Title</th>
      <th scope="col">img</th>
      <th scope="col">files</th>
      <th scope="col">Status</th>
    </tr>
  </thead>
  <tbody>
    <%= render @posts %>
  </tbody>
</table>